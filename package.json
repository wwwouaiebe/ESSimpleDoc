{
  "type": "module",
  "name": "essimpledoc",
  "description": "A simple documentation generator for ES modules",
  "main": "bin/index.js",
  "bin": {
    "esd": "./src/index.js"
  },
  "version": "1.3.5",
  "keywords": [
    "documentation",
    "es",
    "module",
    "simple"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "latest",
    "eslint": "latest",
    "grunt": "latest",
    "grunt-eslint": "latest"
  },
  "dependencies": {
    "@babel/parser": "latest",
    "@babel/traverse": "latest",
    "marked": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwwouaiebe/ESSimpleDoc.git"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "debug": "node node_modules/grunt/bin/grunt",
    "test" : "node src/index.js --src=src --dest=docs --validate --launch"
  },
  "author": "wwwouaiebe",
  "license": "GPL-3.0",
  "sources": "https://github.com/wwwouaiebe/ESSimpleDoc",
  "homepage": "https://github.com/wwwouaiebe/ESSimpleDoc#readme"
}
